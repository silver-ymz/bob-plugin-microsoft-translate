{
  "identifier": "com.silver-ymz.microsoft-translate",
  "category": "translate",
  "version": "0.1.2",
  "name": "microsoft-translate",
  "summary": "Bob plugin for microsoft translation.",
  "author": "silver-ymz",
  "appcast": "https://raw.githubusercontent.com/silver-ymz/bob-plugin-microsoft-translate/master/src/appcast.json",
  "homepage": "https://github.com/silver-ymz/bob-plugin-microsoft-translate",
  "icon": "icon.png",
  "minBobVersion": "0.5.0",
  "options": [
    {
      "identifier": "cache",
      "type": "menu",
      "title": "缓存",
      "defaultValue": "disable",
      "menuValues": [
        {
          "title": "开启",
          "value": "enable"
        },
        {
          "title": "关闭",
          "value": "disable"
        }
      ]
    },
    {
      "identifier": "endpoint",
      "type": "text",
      "defaultValue": "https://api.cognitive.microsofttranslator.com/",
      "title": "服务器地址"
    },
    {
      "identifier": "key",
      "type": "text",
      "title": "Secret Key"
    },
    {
      "identifier": "location",
      "type": "text",
      "title": "区域"
    }
  ]
}
